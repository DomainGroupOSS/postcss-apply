---
root: true
parser: "babel-eslint"
extends: "airbnb-base"
env:
  jest: true

rules:
  no-use-before-define:
    - error
    - functions: false
  comma-dangle:
    - error
    - arrays: "always-multiline"
      objects: "always-multiline"
      imports: "always-multiline"
      exports: "always-multiline"
      functions: ignore
